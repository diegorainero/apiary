    "@types/react-router-dom": "^5.1.6",
    "autoprefixer": "^9.8.6",
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "craco": "0.0.3",
    "express": "4.17.1",
    "helmet": "3.22.0",
    "knex": "0.20.13",
    "postcss": "^7.0.35",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "sqlite3": "^5.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "concurrently": "5.1.0",
    "mocha": "^8.2.1",
    "nodemon": "2.0.3",
    "react-scripts": "^3.4.4",
    "typescript": "3.8.3"
  },
  "scripts": {
